{% stylesheets filter='cssrewrite'
    '@OpenWidePublishAgendaBundle/Resources/public/css/2.3.1/fullcalendar.min.css'
    '@OpenWidePublishAgendaBundle/Resources/public/css/bootstrap.min.css'
    '@OpenWidePublishAgendaBundle/Resources/public/css/bootstrap-theme.min.css'
%}
<link rel="stylesheet" type="text/css" href="{{ asset_url }}" media="screen" />
{% endstylesheets %}

{% stylesheets filter='cssrewrite'
    '@OpenWidePublishAgendaBundle/Resources/public/css/2.3.1/fullcalendar.print.css'
%}
<link rel="stylesheet" type="text/css" href="{{ asset_url }}" media="print" />
{% endstylesheets %}

{% if type == 'mini' %}
    {% stylesheets filter='cssrewrite'
        '@OpenWidePublishAgendaBundle/Resources/public/css/fullcalendar_mini.css'
    %}
<link rel="stylesheet" type="text/css" href="{{ asset_url }}" media="screen" />
{% endstylesheets %}
{% else %}
    {% stylesheets filter='cssrewrite'
        '@OpenWidePublishAgendaBundle/Resources/public/css/fullcalendar.css'
    %}
    <link rel="stylesheet" type="text/css" href="{{ asset_url }}" media="screen" />
    {% endstylesheets %}
{% endif %}

{% stylesheets filter='cssrewrite'
    '@OpenWidePublishAgendaBundle/Resources/public/css/open_wide_publish_agenda.css'
%}
<link rel="stylesheet" type="text/css" href="{{ asset_url }}" media="screen" />
{% endstylesheets %}

<style type="text/css">
    @import url("{{ asset('extension/ezwt/design/standard/stylesheets/websitetoolbar.css') }}");
</style>

{{ ez_legacy_render_css() }}
